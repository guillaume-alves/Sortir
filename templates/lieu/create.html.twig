{% extends 'base.html.twig' %}

{% block body %}
<div class="col-md-9 my-auto mx-auto">
    {{ form_start(form) }}
    <div class="row">
        {{ form_row(form.ville) }}
    </div>
    <div class="row">
        <div class="col-md-6">
            {{ form_row(form.nom) }}
        </div>
        <div class="col-md-6">
            {{ form_row(form.rue) }}
        </div>
    </div>
    <div class="row">
        <div class="col-md-6">
            {{ form_row(form.latitude) }}
        </div>
        <div class="col-md-6">
            {{ form_row(form.longitude) }}
        </div>
        {{ form_rest(form) }}
    </div>
    <div class="d-grid gap-2">
        <button class="btn btn-primary" formnovalidate>Envoyer</button>
    </div>
</div>
{% endblock %}

{% block title %}

{% endblock %}

